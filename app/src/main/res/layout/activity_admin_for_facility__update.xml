<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin_for_availability_insert">
    <View
        android:id="@+id/view11"
        android:layout_width="wrap_content"
        android:layout_height="88dp"
        android:background="@drawable/head"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view12"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:background="@drawable/ic_baseline_account_circle_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <View
        android:id="@+id/view13"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/ic_baseline_menu_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view14"
        android:layout_width="154dp"
        android:layout_height="41dp"
        android:layout_marginEnd="240dp"
        android:layout_marginRight="240dp"
        android:background="@drawable/untitled_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.005" />

    ...

    <View
        android:id="@+id/view15"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="112dp"
        android:layout_marginRight="112dp"
        android:background="@drawable/ic_baseline_search_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <View
        android:id="@+id/view16"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="196dp"
        android:layout_marginRight="196dp"
        android:background="@drawable/ic_baseline_shopping_cart_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <View
        android:id="@+id/view17"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="276dp"
        android:layout_marginRight="276dp"
        android:background="@drawable/ic_baseline_location_on_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <View
        android:id="@+id/view18"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="352dp"
        android:layout_marginRight="352dp"
        android:background="@drawable/ic_baseline_home_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.078" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/archivo_black"
        android:text="@string/add_hotel_form"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view16"
        app:layout_constraintVertical_bias="0.036" />

    <Button
        android:id="@+id/button18"
        android:layout_width="112dp"
        android:layout_height="41dp"
        android:layout_marginTop="140dp"
        android:background="@android:color/holo_purple"
        android:fontFamily="sans-serif"
        android:text="Discription"
        android:textColor="@android:color/background_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button15"
        android:layout_width="117dp"
        android:layout_height="41dp"
        android:layout_marginTop="140dp"
        android:background="@color/hotel_main_cat_btn"
        android:drawableLeft="@drawable/ic_baseline_arrow_drop_down_24"
        android:fontFamily="@font/archivo_black"
        android:text="@string/facilities"
        android:textColor="@android:color/background_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.409"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button17"
        android:layout_width="107dp"
        android:layout_height="41dp"
        android:layout_marginTop="140dp"
        android:background="@android:color/holo_purple"
        android:fontFamily="sans-serif"
        android:text="@string/availability"
        android:textColor="@android:color/background_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button16"
        android:layout_width="58dp"
        android:layout_height="41dp"
        android:layout_marginTop="140dp"
        android:background="@android:color/holo_purple"
        android:fontFamily="sans-serif"
        android:text="@string/rules"
        android:textColor="@android:color/background_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.964"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="192dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp">

            <TextView
                android:id="@+id/FoodAndDrinkT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Food And Drink"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/foodsAndDrinks"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background_normal"
                android:gravity="top|start"
                android:inputType="textMultiLine"
                android:lines="15"
                android:maxLines="20"
                android:minLines="6"
                android:scrollbars="vertical"
                android:scrollHorizontally="false" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Outdoors"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/outDoor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background_normal"
                android:gravity="top|start"
                android:inputType="textMultiLine"
                android:lines="15"
                android:maxLines="20"
                android:minLines="6"
                android:scrollbars="vertical"
                android:scrollHorizontally="false" />

            <TextView
                android:id="@+id/CleaningServicesT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cleaning Services"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/CleaningServices"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background_normal"
                android:gravity="top|start"
                android:inputType="textMultiLine"
                android:lines="15"
                android:maxLines="20"
                android:minLines="6"
                android:scrollbars="vertical"
                android:scrollHorizontally="false" />

            <TextView
                android:id="@+id/SafetyFeaturesT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Safety Features"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/SafetyFeatures"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background_normal"
                android:gravity="top|start"
                android:inputType="textMultiLine"
                android:lines="15"
                android:maxLines="20"
                android:minLines="6"
                android:scrollbars="vertical"
                android:scrollHorizontally="false" />


            <TextView
                android:id="@+id/HealthWellnessFacilitiesT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Health And Wellness Facilities"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/HealthAndWellnessFacilities"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background_normal"
                android:gravity="top|start"
                android:inputType="textMultiLine"
                android:lines="15"
                android:maxLines="20"
                android:minLines="6"
                android:scrollbars="vertical"
                android:scrollHorizontally="false" />
            <TextView
                android:id="@+id/InternetT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Internet"
                android:paddingTop="12dp"
                android:textSize="20sp" />
            <EditText
                android:id="@+id/Internet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background_normal"
                android:gravity="top|start"
                android:inputType="textMultiLine"
                android:lines="15"
                android:maxLines="20"
                android:minLines="6"
                android:scrollbars="vertical"
                android:scrollHorizontally="false" />

            <TextView
                android:id="@+id/ParkingT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Parking"
                android:paddingTop="12dp"
                android:textSize="20sp" />
            <EditText
                android:id="@+id/Parking"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background_normal"
                android:gravity="top|start"
                android:inputType="textMultiLine"
                android:lines="6"
                android:maxLines="10"
                android:minLines="4"
                android:scrollbars="vertical"
                android:scrollHorizontally="false" />

            <Button
                android:id="@+id/btn_delete"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                android:background="@color/update_btn"
                android:fontFamily="@font/archivo_black"
                android:text="Update"
                android:textColor="@color/common_google_signin_btn_text_dark_default"
                android:textSize="18sp" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>